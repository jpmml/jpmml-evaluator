				<ScoreDistribution value="drugB" recordCount="76.0"/>
				<ScoreDistribution value="drugC" recordCount="1.0"/>
				<ScoreDistribution value="drugX" recordCount="0.0"/>
				<ScoreDistribution value="drugY" recordCount="0.0"/>
			</SimpleRule>
