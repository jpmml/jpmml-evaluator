<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PMML xmlns="https://www.dmg.org/PMML-4_1" version="4.1">
	<Header copyright="MyCopyright">
		<Application name="MyApplication" version="1.0"/>
	</Header>
	<DataDictionary numberOfFields="7">
