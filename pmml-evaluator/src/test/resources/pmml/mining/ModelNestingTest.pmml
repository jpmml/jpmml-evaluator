<?xml version="1.0" encoding="UTF-8" ?>
<PMML xmlns="https://www.dmg.org/PMML-4_3" version="4.3">
	<Header/>
	<DataDictionary>
		<DataField name="input" optype="continuous" dataType="double"/>
	</DataDictionary>
	<MiningModel functionName="regression">
		<MiningSchema>
			<MiningField name="input"/>
		</MiningSchema>
		<Segmentation multipleModelMethod="modelChain">
			<Segment>
				<True/>
				<RegressionModel functionName="regression">
					<MiningSchema>
						<MiningField name="input"/>
					</MiningSchema>
					<Output>
						<OutputField name="output" feature="predictedValue"/>
						<OutputField name="report(output)" x-reportField="output" feature="x-report"/>
					</Output>
					<RegressionTable intercept="0.0">
						<NumericPredictor name="input" coefficient="2.0"/>
					</RegressionTable>
				</RegressionModel>
			</Segment>
		</Segmentation>
	</MiningModel>
</PMML>
